{
  "@context": ["http://schema.org/", {"webapi": "http://actions.semantify.it/vocab/"}],
    "@type": "Action",
    "name": "Creates new events objects under an organization and returns it as event.",
    "actionStatus": {"@type": "ActionStatusType", "@id": "http://schema.org/PotentialActionStatus"},
  "webapi:authentication": {
        "@type": "FormBasedAuthentication",
        "value-input": {
            "@type":"PropertyValueSpecification",
            "valueRequired": true
        },
      "webapi:sentIn": {"@id": "webapi:HTTPMessageHeader"},
        "name-input": {
            "@type":"PropertyValueSpecification",
            "valueRequired": true
        }
    },
    "result": {
        "@type": "Thing",
        "name-input":{
            "@type":"PropertyValueSpecification",
            "valueRequired": true
        }
    },
    "target": {
        "@type":"EntryPoint",
        "urlTemplate": "https://www.eventbriteapi.com/v3/{id}/events/",
        "encodingType": {"@value": "application/ld+json"},
        "contentType": {"@value": "application/ld+json"},
        "httpMethod": "POST"
    }
}